<?xml version="1.0"?>

<!DOCTYPE suppressions PUBLIC
     "-//Puppy Crawl//DTD Suppressions 1.0//EN"
     "http://www.puppycrawl.com/dtds/suppressions_1_0.dtd">

<suppressions>
	<!-- suppress all checks for JUnit files (*Test.java) -->
	<!-- <suppress checks="[a-zA-Z0-9]*" files=".+Test.java" /> -->
	
	<!-- suppress all javadoc checks for JUnit files (*Test.java) -->
  	<suppress checks="Javadoc*" files=".+Test.java"/>
  	
  	<!-- suppress magic number checks for JUnit files (*Test.java) -->
  	<suppress checks="MagicNumberCheck" files=".+Test.java"/>
</suppressions>
